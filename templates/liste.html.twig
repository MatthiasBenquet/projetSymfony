{% extends 'base.html.twig' %}

{% block body %}
	<div class="row">
		<div class="col-md-12">
			<h1>{% block h1 %}{% endblock %}</h1>
		</div>
	</div>

	<div class="row">
		<div class="col-md-12">
			<div class="list-group">
				{% block elements %}{% endblock %}
			</div>
		</div>
	</div>

	<div class="row">
		<div class="col-md-12">
			<div id="boutons" align="center">
				{% block boutons %}{% endblock %}
			</div>
		</div>
	</div>
{% endblock %}